#include <stdio.h>
int main()
{
    int num1,num2,num3;
    printf("Enter three numbers:\n");
    scanf("%d\n%d\n%d",&num1,&num2,&num3);
    if(num1>num2 && num1>num3)
    {
        printf("%d is the largest.",num1);
    }
    else if (num2>num3 && num2>num1)
    {
        printf("%d is largest.",num2);
    }
    else
        {
            printf("%d is largest.",num3);
        }
    return 0;
}
=====================================================================================================================================
Output :
Enter three numbers:
7 is largest.
